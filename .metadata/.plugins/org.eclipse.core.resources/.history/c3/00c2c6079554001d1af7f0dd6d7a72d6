package assigmentJava;

public class PatternPrograms {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int i =1;i <=5;i++) {
			int num=i;
			for (int j =5; j>=i;j--) {
				System.out.println(num);
				num = num+5-j;
			}
			System.out.println();
		}
		
		
		
	}

}
